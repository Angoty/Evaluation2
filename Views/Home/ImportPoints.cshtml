@{
    ViewData["Title"] = "Import des points";
    ViewData["Profil"] = "Admin";
}
<div class="pd-ltr-20 xs-pd-20-10">
    <div class="min-height-200px">
        <div class="pd-20 card-box mb-30">
            <div class="clearfix">
                <div class="pull-left">
                    <h4 class="text-blue h4">Import des points</h4>
                </div>
            </div>
            <form action="@Url.Action("TraitementImportPoints", "Home")"  method="post" enctype = "multipart/form-data">
                <div class="form-group">
                    <label>Import des points pour le classement:</label>
                    <input type="file" class="form-control-file form-control height-auto" name="points">
                </div>
                <button type="submit" class="btn btn-primary">Importer</button>
            </form>
        </div>
    </div>
</div>